@model PayslipImportViewModel



<div class="flex-box form-function bg-PayslipImportBriefInfo">
    <div class="items">
        <div class="form-group">
            <label asp-for="SalaryYearMonth"></label>
            <input type="text" asp-for="SalaryYearMonth"
                   data-val="false" formnovalidate="formnovalidate"
                   class="form-control-plaintext cancel" />
        </div>
    </div>
    <div class="items">
        <div class="form-group">
            <label asp-for="NumberOfEmployees"></label>
            <input type="text" asp-for="NumberOfEmployees"
                   data-val="true" required
                   class="form-control-plaintext cancel" />
        </div>
    </div>
    <div class="items">
        <div class="form-group">
            <label asp-for="Remark"></label>
            <input type="text" asp-for="Remark"
                   data-val="true" required
                   class="form-control-plaintext cancel" />
        </div>
    </div>
    <div class="items">
        <div class="form-group">
            <div class="form-group">
                <label asp-for="ImportedDate"></label>
                <input type="text" asp-for="ImportedDate"
                       data-val="false" formnovalidate="formnovalidate"
                       class="form-control-plaintext cancel" />
            </div>
        </div>
    </div>


</div>